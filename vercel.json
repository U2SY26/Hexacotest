{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "installCommand": "npm install",
  "framework": null,
  "cleanUrls": false,
  "trailingSlash": false,
  "routes": [
    { "src": "/api/(.*)", "dest": "/api/$1" },
    { "src": "/google2e375c8dd7f15496.html", "headers": { "Content-Type": "text/html" }, "dest": "/google2e375c8dd7f15496.html" },
    { "src": "/ads.txt", "headers": { "Content-Type": "text/plain" }, "dest": "/ads.txt" },
    { "src": "/robots.txt", "headers": { "Content-Type": "text/plain" }, "dest": "/robots.txt" },
    { "src": "/sitemap.xml", "headers": { "Content-Type": "application/xml" }, "dest": "/sitemap.xml" },
    { "src": "/hexagon.svg", "dest": "/hexagon.svg" },
    { "src": "/assets/(.*)", "dest": "/assets/$1" },
    { "src": "^/(?!.*\\.).*$", "dest": "/index.html" }
  ]
}
